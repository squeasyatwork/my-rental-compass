generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model data_by_suburb {
  suburb          String   @id(map: "data_by_suburb_pk")
  lga             String?
  postcode        Int?
  average_rent    Decimal? @db.Decimal
  ptv_stop_count  Int?
  openspace_count Int?
  crash_count     Int?
  crime_count     Int?
  bus_stop        Int?
  tram_stop       Int?
  train_station   Int?
}

model liveability_data {
  suburb            String   @id(map: "liveability_data_pk")
  rent_score        Decimal? @db.Decimal
  transport_score   Decimal? @db.Decimal
  openspace_score   Decimal? @db.Decimal
  saferoads_score   Decimal? @db.Decimal
  crime_score       Decimal? @db.Decimal
  liveability_score String?
  average_rent      Decimal? @db.Decimal
  ptv_stop_count    Int?
  openspace_count   Int?
  crash_count       Int?
  crime_count       Int?
}

model uni_suburbs {
  university     String  @id(map: "uni_suburbs_pk")
  uni_suburb     String?
  nearby_suburbs String?
}
